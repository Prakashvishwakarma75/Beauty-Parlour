<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Beauty.Admin</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/jquery.dataTables.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Beauty.Admin.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">

    @*@* for  Alert massage *@
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto|Varela+Round">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
 
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">
                    <img width="120" class="rounded" src="data:image/png;base64," />
                </a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-white btn" onclick="OpenMakeup()" href="#" style="background-color:#ee4f48;height:44px">
                                <i class="fa fa-paint-brush" aria-hidden="true" style="font-size: 28px; color:white;"></i> <span>Maekup</span>
                            </a>
                        </li>
                       @*  <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li> *@
                    </ul>
                </div>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                        <div></div>
                        <div class="profile">
                            <div class="user">
                                <p>probablykat66</p>
                            </div>
                            <div class="img-box">
                                <img src="https://i.postimg.cc/BvNYhMHS/user-img.jpg" alt="some user image">
                            </div>
                        </div>
                        <div class="menu">
                            <ul>
                                <li><a href="#"><i class="ph-bold ph-user"></i>&nbsp;Profile</a></li>
                                <li><a href="#"><i class="ph-bold ph-envelope-simple"></i>&nbsp;Inbox</a></li>
                                <li><a href="#"><i class="ph-bold ph-gear-six"></i>&nbsp;Settings</a></li>
                                <li><a href="#"><i class="ph-bold ph-question"></i>&nbsp;Help</a></li>
                                <li><a href="#"><i class="ph-bold ph-sign-out"></i>&nbsp;Sign Out</a></li>
                            </ul>
                        </div>
                </div>
            </div>
        </nav>
    </header>
    <!-- Modal HTML -->
    <div id="myModal" class="modal fade">
        <div class="modal-dialog modal-confirm">
            <div class="modal-content">
                <div class="modal-header flex-column">
                    <div class="icon-box">
                        <i class="material-icons">&#xE5CD;</i>
                    </div>
                    <h4 class="modal-title w-100">Are you sure?</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                </div>
                <div class="modal-body">
                    <p>Do you really want to delete these records? This process cannot be undone.</p>
                </div>
                <div class="modal-footer justify-content-center">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-danger">Delete</button>
                </div>
            </div>
        </div>
    </div>
    <div class="px-2" style="overflow:hidden">
        <main role="main" class="pb-3">
            <div class="row">
                <div class="col-md-2">
                    <div class="shadow-lg p-1 rounded" style="background-color:#ee4f48;height:min-content">
                        <ul class="list-group">
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenHome()" href="#"><i class="fa fa-home" style="font-size:28px;color:white"></i>    Home </a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenAbout()" href="#"><i class="fa fa-info-circle" style="font-size:28px;color:white"></i>    About </a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenService()" href="#"><i class="fa fa-wrench" aria-hidden="true" style="font-size:28px;color:white"></i>  Services</a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenNews()" href="#"><i class="fas fa-blog" style="font-size: 28px; color: white;"></i>  News</a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenContact()" href="#"><i class="fa fa-address-book" aria-hidden="true" style="font-size: 28px; color: white;"></i> Contact</a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenPrice()" href="#"><i class="fa fa-money" aria-hidden="true" style="font-size: 28px; color: white;"></i> Price</a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenMakeup()" href="#"><i class="fa fa-paint-brush" aria-hidden="true" style="font-size: 28px; color: white;"></i> Maekup</a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenBanner()" href="#"><i class="fa fa-picture-o" aria-hidden="true" style="font-size: 28px; color: white;"></i> Banner</a></li>
                            @* <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenTeam()" href="#"><i class="fa fa-address-book" aria-hidden="true" style="font-size: 28px; color: white;"></i> Team</a></li> *@
                           @*  <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenClientOpen()" href="#"><i class="fa fa-address-book" aria-hidden="true" style="font-size: 28px; color: white;"></i> ClientOpen</a></li>
                            <li class="list-group-item" style="background-color:#ee4f48;color:white"><a onclick="OpenClientBrands()" href="#"><i class="fa fa-address-book" aria-hidden="true" style="font-size: 28px; color: white;"></i> Brands</a></li> *@
                          </ul>
                    </div>
                </div>
                <div class="col-md-9">
                    @RenderBody()
                </div>
            </div>
        </main>
    </div>

    <footer class="border-top footer text-muted bg-white">
        <div class="container">
            &copy; 2023 - Beauty.Admin - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/js/jquery-3.7.0.min.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/jquery.dataTables.js" asp-append-version="true"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <script>
        $(document).ready(function () {
            $('.DataTable').DataTable({
                "paging": true,
                "pageLength": 6,
                "lengthChange": false,
            });
        });
        let profile = document.querySelector('.profile');
        let menu = document.querySelector('.menu');

        profile.onclick = function () {
            menu.classList.toggle('active');
        }
        function OpenAbout() {
            window.location.href = "/beauty/beautyaboutus";
        }
        function OpenService() {
            window.location.href = "/beauty/beautyservice";
        }
        function OpenContact() {
            window.location.href = "/beauty/beautycontactus";
        }
        function OpenNews() {
            window.location.href = "/beauty/beautynews";
        }
        function OpenPrice() {
            window.location.href = "/beauty/beautyprice";
        } 
        function OpenMakeup() {
            window.location.href = "/beauty/beautymakeup";
        }
        function OpenBanner() {
            window.location.href = "/beauty/beautybanner";
        }
        function OpenTeam() {
            window.location.href = "/beauty/beautyteam";
        }
        function OpenClientOpen() {
            window.location.href = "/beauty/beautyclientopen";
        }
        function OpenClientBrands() {
            window.location.href = "/beauty/beautybrands";
        }
    </script>
</body>
</html>
